{
  "author": "BitWiki支持团队",
  "description": "A permanently free and open source knowledge base for cyber security | 一个永久免费 、开源的网络安全知识库",
  "language": "zh-hans",
  "links": {
    "sidebar": {
      "OneBit安全团队": "https://team.1bit.win/",
      "GitHub": "https://github.com/BitWiki/bitwiki.github.io"
    }
  },
  "plugins": [
    "codeblock-filename",
    "summary",
    "chapter-fold",
    "copy-code-button",
    "-sharing",
    "-lunr",
    "search-pro",
    "splitter",
    "github-buttons",
    "insert-logo",
    "custom-favicon",
    "popup",
	"back-to-top-button",
	"advanced-emoji",
    "theme-comscore",
    "rss",
    "hide-element",
	"anchor-navigation-ex"
  ],
  "pluginsConfig": {
    "github-buttons": {
      "repo": "Bitwiki/bitwiki.github.io",
      "types": [
        "star",
        "watch",
        "fork"
      ],
      "size": "small"
    },
    "favicon": "../images/favicon.ico",
    "insert-logo": {
      "url": "../images/BitWiki.png",
      "style": "background: none; max-height: 60px; min-height: 60px"
    },
    "rss": {
      "title": "BitWiki",
      "description": "A permanently free and open source knowledge base for cyber security | 一个永久免费 、开源的网络安全知识库",
      "author": "BitWiki支持团队",
      "feed_url": "https://wiki.1bit.win/doc/rss",
      "site_url": "https://wiki.1bit.win/",
      "managingEditor": "team@onebit.tk",
      "webMaster": "team@onebit.tk",
      "categories": [
        "gitbook"
      ]
    },
    "hide-element": {
      "elements": [".gitbook-link"]
    },
	"anchor-navigation-ex":{
		"showLevel": false,
		"showGoTop":true
	}
  }
}